{
    "name": "nabeel/vacentral",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">= 7.0.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "atoum/atoum": "^3.2",
        "atoum/stubs": "^2.5"
    },
    "archive": {
        "exclude": [
            ".idea",
            "tests",
            ".travis.yml",
            "Makefile"
        ]
    },
    "autoload": {
        "psr-4": {
            "VaCentral\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "config": {
        "sort-packages": true
    }
}
