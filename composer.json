{
    "name": "nabeel/vacentral",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">= 7.0.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.4.0",
        "phpunit/phpunit-mock-objects": "4.0.4",
        "scriptfusion/phpunit-immediate-exception-printer": "1.3.0"
    },
    "autoload": {
        "psr-4": {
            "VaCentral\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
